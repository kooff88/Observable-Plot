{
  singleQuote: true,
  trailingComma: 'none',
  printWidth: 100,
  tabWidth: 2,
  useTabs: false,
  semi: true,
  quoteProps: 'as-needed',
  stylelintIntegration: true,
  insertPragma: false,
  arrowParens: 'avoid',
  proseWra: 'preserve',
  eslintIntegration: true,
  overrides: [
    {
      files: '.prettierrc',
      options: { parser: 'json' }
    }
  ]
}
